#include <stdio.h>
#include <math.h> //uso da biblioteca math para fazer a potênciação
#include <locale.h>

//Um programa que solicita e lê um número, calcula e exibe o seu cubo.

int main(){
	setlocale(LC_ALL, "Portuguese");
	float n;

	printf("Digite um número:");
		scanf("%f", &n);

	printf("O cubo de %.0f é %.0f", n, pow(n,3));
	return(0);
}
